class Account 
{
	private double balance;
	public Account(double balance){
		this.balance=balance;
	}
	public double getBalance(){
		return balance;
	}
	public boolean deposit(double amount){
		balance+=amount;
		return true;
	}
	public boolean withdraw(double amt){
		if(amt<=balance){
		balance-=amot;
		return true;
		}
		else{
			return false;
		}
	}
}
